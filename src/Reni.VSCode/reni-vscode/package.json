{
  "name": "reni-vscode",
  "publisher": "ixu",
  "displayName": "reni-vscode",
  "description": "Reni Language support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "reni",
        "extensions": [
          ".reni"
        ],
        "configuration": "./TextMate/language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "reni",
        "scopeName": "source.reni",
        "path": "./TextMate/reni.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "electron": "31.3",
    "electron-edge-js": "31.3"
  }
}
