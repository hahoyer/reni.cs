{
  "version": "1.0.0-*",
  "compilationOptions": {
    "emitEntryPoint": true
  },
  "dependencies": {
    "ReniCompilerService": "1.0.0"
  },
  "commands": {
    "web": "Microsoft.AspNet.Server.Kestrel"
  },
  "frameworks": {
    "dnx451": {
      "dependencies": {
        "ReniUI": "1.0.0-*"
      },
      "frameworkAssemblies": {
        "System.Web.Extensions": "4.0.0.0"
      }
    }
  },
  "exclude": [
    "wwwroot",
    "node_modules"
  ],
  "publishExclude": [
    "**.user",
    "**.vspscc"
  ],
  "entryPoint": "ReniCompilerService",
  "loadable": false
}